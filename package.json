        "graphql-tools": "^5.0.0",
        "ioredis": "^4.27.6",
        "mongoose": "^5.12.14",
        "nest-winston": "^1.5.0",
        "nestjs-graphql-pagination": "^1.0.0",
        "nestjs-redis": "^1.3.3",
        "pg": "^8.6.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "tweetnacl": "^1.0.3",
        "winston": "^3.3.3",
        "winston-transport": "^4.4.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.0.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.6.18",
        "@types/cron": "^1.7.2",
        "@types/express": "^4.17.12",
        "@types/jest": "25.1.4",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^13.13.52",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "^2.23.0",
        "@typescript-eslint/parser": "^2.23.0",
        "apollo-server-fastify": "^3.0.0-preview.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.23.4",
        "jest": "^25.1.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "ts-jest": "25.2.1",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.9.10"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
